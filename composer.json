{
    "name": "painless-php/string",
    "description": "String helper functionality for PHP",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "spatie/phpunit-watcher": "^1.22",
        "vlucas/phpdotenv": "^4.1",
        "phpbench/phpbench": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "PainlessPHP\\String\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
